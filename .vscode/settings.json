{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "cSpell.words": [
    "nuxt",
    "nuxtjs",
    "vueuse",
    "unocss",
    "attributify",
    "pinia",
    "taze",
    "antfu",
    "iconify",
    "nuxi",
    "unplugin"
  ]
}
